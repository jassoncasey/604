

data Breakfast = { Bacon Nat | Eggs Char, Nat | Ham }

inc :: Nat -> Nat;
inc k = k + 1;

square :: Nat -> Nat;
square a = a * a;

dontCare :: Nat -> Nat -> Nat;
dontCare a b = 3;

saidYouCared :: Nat;
saidYouCared = dontCare (square 23) 2;

aMajorCase :: Breakfast -> Nat;
aMajorCase item = case item of
  { Bacon a -> a; Eggs a b -> 1729; Ham -> 4 };